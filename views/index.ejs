<!DOCTYPE html>
<html lang="es">
<head>    
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/css/font-awesome.css">
    <link rel="stylesheet" href="/css/g_style.css">   
</head>
<body>
    <div id="contenido">
        <header>    
            <img id="logo" src="/img/tropi-rents.png"/>             
        </header> 
        <div id="navegacion">
            <nav>
                <span><i class="fa fa-bed"></i>Rentas de casas</span>                
                <span><i class="fa fa-taxi"></i>Rentas en la ciudad</span>                            
                <span><i class="fa fa-building"></i>Pasadias en piscinas.</span>    
                <span><i class="fa fa-building"></i>Locales para fiestas.</span>    
                <span><i class="fa fa-taxi"></i>Servicio de transporte.</span>    
                <span><i class="fa fa-building"></i>Tours por la isla</span>    
            </nav>
        </div>

        <div id="transicion1">
            
        </div>
        <div id="cartel">
            <p id="rotulos" >...Esto...</p>
        </div>       
        
    </div>    
    <script>
        document.onload=function()
        {
         var rotulo=document.getElementById('rotulos');
         var estado=0;
        //var timer=setInterval(cambiar,2000);
               
        function cambiar()
        
        {          
            
            switch (estado)
             {
                case 1:
                    rotulo.innerHTML="...Esto..."
                    break;
                case 2:
                rotulo.innerHTML="...es un letrero..."
                    break;                    
                case 3:
                rotulo.innerHTML="...que sigue cambiando..."
                    break;    

             }
             if (estado>=4)
             {estado=0}
        }
        rotulo.onanimationiteration=function()
        {
            estado++;
            console.log("Escribir",estado);
            cambiar();
        }
        
        }
    </script>
    <noscript>
        Activa el interprete de Javascript del navegador
    </noscript>
</body>
</html>